set $i3-new-workspace .config/i3/new-workspace/i3-new-workspace.sh

bindsym $mod+n exec --no-startup-id $i3-new-workspace
bindsym $mod+e exec --no-startup-id $i3-new-workspace

bindsym $mod+Shift+n exec --no-startup-id $i3-new-workspace -m
bindsym $mod+Shift+e exec --no-startup-id $i3-new-workspace -m

bindsym $mod+Control+n exec --no-startup-id $i3-new-workspace -c
bindsym $mod+Control+e exec --no-startup-id $i3-new-workspace -c
